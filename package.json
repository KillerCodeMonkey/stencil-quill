{
  "name": "stencil-quill",
  "version": "9.0.0",
  "description": "Native web component for quill editor",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.js",
  "es2017": "dist/esm/index.js",
  "jsnext:main": "dist/esm/index.js",
  "types": "./dist/types/components.d.ts",
  "collection": "./dist/collection/collection-manifest.json",
  "unpkg": "dist/quill-components/quill-components.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "stencil build --docs",
    "build:deploy": "stencil build && cp ./src/index.html ./dist && gh-pages -d ./dist",
    "prepublishOnly": "npm run build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll"
  },
  "keywords": [
    "stencil",
    "richtext",
    "quill",
    "quilljs",
    "webcomponent",
    "web-component",
    "component",
    "wysiwyg",
    "native"
  ],
  "peerDependencies": {
    "quill": "^1.3.6"
  },
  "devDependencies": {
    "@stencil/core": "^4.12.0",
    "@types/jest": "^27.0.3",
    "@types/quill": "^1.3.10",
    "canvas": "^2.11.2",
    "gh-pages": "5.0.0",
    "jest": "^27.0.3",
    "jest-cli": "^27.4.5",
    "jsdom": "^22.1.0",
    "prettier": "^2.8.8",
    "puppeteer": "^20.9.0",
    "quill": "^1.3.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KillerCodeMonkey/stencil-quill.git"
  },
  "author": "Bengt Wei√üe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KillerCodeMonkey/stencil-quill"
  },
  "homepage": "https://github.com/KillerCodeMonkey/stencil-quill"
}
