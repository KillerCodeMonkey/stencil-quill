{
  "name": "stencil-quill",
  "version": "11.0.1",
  "description": "Native web component for quill editor",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.js",
  "es2017": "dist/esm/index.js",
  "jsnext:main": "dist/esm/index.js",
  "types": "./dist/types/components.d.ts",
  "collection": "./dist/collection/collection-manifest.json",
  "unpkg": "dist/quill-components/quill-components.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "stencil build --docs",
    "build:deploy": "stencil build && cp ./src/index.html ./dist && gh-pages -d ./dist",
    "prepublishOnly": "npm run build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll"
  },
  "keywords": [
    "stencil",
    "richtext",
    "quill",
    "quilljs",
    "webcomponent",
    "web-component",
    "component",
    "wysiwyg",
    "native"
  ],
  "peerDependencies": {
    "quill": "^2.0.0"
  },
  "devDependencies": {
    "@stencil/core": "^4.31.0",
    "@types/jest": "^29.5.14",
    "canvas": "^3.1.0",
    "gh-pages": "^6.3.0",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "puppeteer": "^24.10.2",
    "quill": "^2.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KillerCodeMonkey/stencil-quill.git"
  },
  "author": "Bengt Wei√üe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KillerCodeMonkey/stencil-quill"
  },
  "homepage": "https://github.com/KillerCodeMonkey/stencil-quill",
  "volta": {
    "node": "22.15.1"
  }
}
