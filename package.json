{
  "name": "stencil-quill",
  "version": "1.0.1",
  "description": "Native web component for quill editor",
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "build:deploy": "stencil build && cp ./src/index.html ./dist && gh-pages -d ./dist",
    "build:publish": "stencil build && cp ./package.json ./dist && cp ./readme.md ./dist",
    "start": "stencil build --dev --watch --serve",
    "test": "jest",
    "test.watch": "jest --watch"
  },
  "keywords": [
    "stencil",
    "richtext",
    "quill",
    "quilljs",
    "webcomponent",
    "web-component",
    "component",
    "wysiwyg",
    "native"
  ],
  "peerDependencies": {
    "quill": "^1.3.6"
  },
  "devDependencies": {
    "@stencil/core": "^0.16.0",
    "@types/jest": "^23.3.2",
    "@types/quill": "^1.3.10",
    "gh-pages": "2.0.1",
    "jest": "^23.6.0",
    "quill": "^1.3.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KillerCodeMonkey/stencil-quill.git"
  },
  "author": "Bengt Wei√üe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KillerCodeMonkey/stencil-quill"
  },
  "homepage": "https://github.com/KillerCodeMonkey/stencil-quill",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
